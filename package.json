{
  "name": "inventory-system",
  "version": "1.0.0",
  "description": "Code Challenge",
  "main": "index.js",
  "scripts": {
    "build-client": "rimraf dist/static && webpack --config webpack.config.babel.js",
    "build-server": "rimraf dist/server && babel ./src/server --out-dir dist/server",
    "dev-client": "webpack-dev-server --config webpack.config.babel.js --inline --hot",
    "build": "npm-run-all build-client build-server",
    "start": "nodemon -r dotenv/config ./src/server/server.js  --ignore '**inventory.json' --exec babel-node",
    "test": "jest --config ./jest.config.json"
  },
